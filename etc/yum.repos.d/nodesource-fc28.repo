[nodesource]
name=Node.js Packages for Fedora Linux 28 - $basearch
baseurl=https://rpm.nodesource.com/pub_10.x/fc/28/$basearch
failovermethod=priority
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/NODESOURCE-GPG-SIGNING-KEY-EL

[nodesource-source]
name=Node.js for Fedora Linux 28 - $basearch - Source
baseurl=https://rpm.nodesource.com/pub_10.x/fc/28/SRPMS
failovermethod=priority
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/NODESOURCE-GPG-SIGNING-KEY-EL
gpgcheck=1
