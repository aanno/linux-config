[connection]
id=${NET_DEV}
type=ethernet
interface-name=$NET_DEV
uuid=$UUID
[ipv4]
# not set -> use DHCP
dns-search=
method=auto
[ipv6]
dns=$IPV6_DNS
gateway=$IPV6_GATEWAY
addr-gen-mode=eui64
# addr-gen-mode=stable-privacy
address1=$IPV6_ADDR1
address2=$IPV6_ADDR2
address3=$IPV6_ADDR3
address4=$IPV6_ADDR4
address5=$IPV6_ADDR5
address6=$IPV6_ADDR6
#dns-search=
method=manual
